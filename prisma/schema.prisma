generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
      url      = env("DATABASE_URL")
}


model ContactUser {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  firstName   String 
  lastName    String
  email       String    @unique
  countryCode Int       // Changed from Number to Int
  phone       Int       // Changed from Number to Int (or could be String)
  message     String
}
